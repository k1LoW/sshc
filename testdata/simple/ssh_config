Host *
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null

Host bastion
  HostName 127.0.0.1
  User k1low
  Port 9022
  IdentityFile ./testdata/id_rsa

Host server
  HostName  172.30.0.3
  User root
  Port 22
  IdentityFile ./testdata/id_rsa
  ProxyCommand ssh -W %h:%p bastion -F ./testdata/simple/ssh_config

Host simple
